<% for(var i=0;i < users.length; i++){%>
<tr>
    <td><%= i+1 %></td>
    <td><%= users[i].assessment %></td>
    <td><%= users[i].date %></td>
    <td><%= users[i].description %></td>
    <td><%= users[i].mark %></td>
    <td><%= users[i].grade %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id %>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fa fa-plus" aria-hidden="true"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
            <span class="text-gradient"><i class="fa fa-times" aria-hidden="true"></i></span>
        </a>
    </td>
</tr>
<% } %>